{
	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "order"
			}
		},{
			"path": "pages/login/index",
			"style": {
				"navigationBarTitleText": "登录/注册",
        "enablePullDownRefresh":true
			}
		}, {
			"path": "pages/myVist/index",
			"style": {
				"navigationBarTitleText": "my-visit"
			}
		}, {
			"path": "pages/personCenter/index",
			"style": {
				"navigationBarTitleText": "person-center"
			}
		}
	],
  "easycom": {
    "autoscan": true,
    "custom": {
      "^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
    }
  },
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8"
	},
	"tabBar": {
		"backgroundColor": "#f2f2f2",
		"borderStyle": "white",
		"selectedColor": "#50bdbb",
		"list": [{
			"iconPath": "./static/order.png",
			"selectedIconPath": "./static/seletIconOrder.png",
			"text": "预约",
			"pagePath": "pages/index/index"
		}, {
			"iconPath": "./static/visit.png",
			"selectedIconPath": "./static/seletIconVisit.png",
			"text": "我的探视",
			"pagePath": "pages/myVist/index"
		}, {
			"iconPath": "./static/personCenter.png",
			"selectedIconPath": "./static/seletIconPersonCenter.png",
			"text": "个人中心",
			"pagePath": "pages/personCenter/index"
		}]
	}
}
